<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>bookstore</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>memberRegisterServlet</servlet-name>
  	<servlet-class>member.memberRegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>memberRegisterServlet</servlet-name>
  	<url-pattern>/memberRegister</url-pattern>
  </servlet-mapping>
  
  <servlet>
   <servlet-name>memberRegisterCheckServlet</servlet-name>
   <servlet-class>member.memberRegisterCheckServlet</servlet-class>
  </servlet>
  <servlet-mapping>
   <servlet-name>memberRegisterCheckServlet</servlet-name>
   <url-pattern>/memberRegisterCheck</url-pattern>
  </servlet-mapping>
</web-app>